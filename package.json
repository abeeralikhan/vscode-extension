{
  "name": "dragme",
  "displayName": "DragMe",
  "description": "Block-based coding",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dragme.helloWorld",
    "onView:dragMe-sidebar",
    "onLanguage:python"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "dragme.helloWorld",
      "title": "Hello World"
    }],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dragMe-sidebar-view",
          "title": "DragMe",
          "icon": "media/gripper.svg"
        }
      ]
    },
    "views": {
      "dragMe-sidebar-view": [
        {
          "type": "webview",
          "id": "dragMe-sidebar",
          "name": "DragMe",
          "icon": "media/gripper.svg",
          "contextualTitle": "DragMe"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
